<div class="container-fluid">
  <div class="row">
    <div class="xs-12 col-md-6 col-md-offset-3">
      <h1>Listing Dive VIPs</h1>

      <table class="table">
        <thead>
          <tr>
            <th>Name</th>
            <th>Email</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @members.each do |member| %>
            <tr>
              <td><%= member.name %></td>
              <td><%= member.email %></td>
              <td><%= link_to 'Delete', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

      <br>
          <div class="well-lg">
            <%= link_to 'Go to Dashboard', dashboard_path, class: "btn btn-default" %>
          </div>
      </div>
    </div>
  </div>